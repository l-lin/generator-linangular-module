{
	"name": "<%= moduleName %>",
	"version": "0.0.1",
	"author": "<%= authorName %>",
	"main": [
        "dist/<%= moduleName %>.js",
        "dist/<%= moduleName %>.min.js",
        "dist/<%= moduleName %>.css",
        "dist/<%= moduleName %>.min.css"
    ],
    "ignore": [
        ".bowerrc",
        ".editorconfig",
        ".git*",
